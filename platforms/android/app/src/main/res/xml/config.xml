<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="1" defaultlocale="fr-FR" id="com.classementparissportifs.mobileapp" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="NativeSettings">
        <param name="android-package" value="com.phonegap.plugins.nativesettings.NativeSettings" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="NativeStorage">
        <param name="android-package" value="NativeStorage" />
    </feature>
    <feature name="AppRate">
        <param name="android-package" value="org.pushandplay.cordova.apprate.AppRate" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="FirebasePlugin">
        <param name="android-package" value="org.apache.cordova.firebase.FirebasePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Classement Paris Sportifs</name>
    <description>
        Gratuit: Comparez les sites de paris sportifs grâce à Classement Paris Sportifs
    </description>
    <author email="top10parissportifs@gmail.com" href="https://www.meilleursparissportifs.com/">
        Comparateur Paris Sportifs
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/mipmap-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="res/icon/android/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="res/icon/android/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/icon/android/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />
    <resource-file src="google-services.json" target="google-services.json" />
    <resource-file src="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" target="res/icon/android/mipmap-xxxhdpi/ic_launcher.png" />
    <splash qualifier="land-ldpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="land-mdpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="land-hdpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="land-xhdpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="land-xxhdpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="land-xxxhdpi" src="res/screens/android/splash-screen-land.png" />
    <splash qualifier="port-ldpi" src="res/screens/android/splash-screen.png" />
    <splash qualifier="port-mdpi" src="res/screens/android/splash-screen.png" />
    <splash qualifier="port-hdpi" src="res/screens/android/splash-screen.png" />
    <splash qualifier="port-xhdpi" src="res/screens/android/splash-screen.png" />
    <splash qualifier="port-xxhdpi" src="res/screens/android/splash-screen.png" />
    <splash qualifier="port-xxxhdpi" src="res/screens/android/splash-screen.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
